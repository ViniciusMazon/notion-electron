  "keywords": [],
  "author": "ViniciusMazon <vnpmazon@gmail.com> (http://github.com/viniciusmazon)",
  "homepage": "https://www.github.com/ViniciusMazon/notion-electron",
  "license": "MIT",
  "devDependencies": {
    "electron": "^19.0.7",
    "electron-builder": "^23.1.0"
  }
}
